{
  "name": "editor-app",
  "version": "0.0.0",
  "scripts": {
    "dev:electron": "tsc -p main.tsconfig.json && electron ./dist",
    "dev:renderer-welcome": "parcel renderer/welcome/src/index.html --port 4100 --out-dir build/debug/renderer/welcome",
    "dev:mix-electron": "wait-on http://localhost:4100 && yarn dev:electron",
    "dev": "concurrently -k \"yarn dev:mix-electron\" \"yarn dev:renderer-welcome\""
  },
  "private": true,
  "dependencies": {
    "chokidar": "^3.3.1",
    "dockerode": "^3.1.0",
    "dockerode-ts": "^0.3.2"
  },
  "devDependencies": {
    "@lv-game-editor/lv-cli": "^1.0.24",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.1.0",
    "electron": "^8.0.1",
    "less": "^3.11.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "~8.3.0",
    "ttab": "^0.6.1",
    "typescript": "~3.7.5",
    "wait-on": "^4.0.0"
  }
}
